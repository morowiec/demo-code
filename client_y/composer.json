{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 8 projects with composer",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Morowiec",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "path",
      "url": "./web/themes/custom/drupal8_theme"
    },
    {
      "type": "package",
      "package": {
        "name": "harvesthq/chosen",
        "version": "1.8.7",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/colorbutton",
        "version": "4.10.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.10.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/panelbutton",
        "version": "4.10.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.10.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/font",
        "version": "4.10.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/font/releases/font_4.11.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "dinbror/blazy",
        "version": "1.8.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/dinbror/blazy/archive/master.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "kenwheeler/slick",
        "version": "1.9.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/kenwheeler/slick/archive/master.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "gdsmith/jquery.easing",
        "version": "1.4.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/gdsmith/jquery.easing/archive/master.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type": "vcs",
      "url": "https://github.com/droptica/codeception-drupal-bootstrap"
    },
    {
      "type": "vcs",
      "url": "https://github.com/droptica/codeception-drupal-users"
    },
    {
      "type": "vcs",
      "url": "https://github.com/droptica/codeception-drupal-storage"
    }
  ],
  "require": {
    "ckeditor/colorbutton": "^4.10",
    "ckeditor/font": "4.10.1",
    "ckeditor/panelbutton": "4.10.1",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "dinbror/blazy": "1.8.1",
    "drupal-composer/drupal-scaffold": "^2.5",
    "drupal-pattern-lab/add-attributes-twig-extension": "^1.0",
    "drupal-pattern-lab/attach-library-twig-extension": "^1.0",
    "drupal-pattern-lab/bem-twig-extension": "^1.0",
    "drupal-pattern-lab/unified-twig-extensions": "^0.1",
    "drupal/components": "^1.0",
    "drupal/address": "^1.4",
    "drupal/admin_toolbar": "^1.24",
    "drupal/adminimal_admin_toolbar": "^1.8",
    "drupal/adminimal_theme": "^1.3",
    "drupal/better_exposed_filters": "^3.0@alpha",
    "drupal/blazy": "^1.0",
    "drupal/block_field": "^1.0",
    "drupal/bootstrap_barrio": "^4.17",
    "drupal/chosen": "^2.6",
    "drupal/ckeditor_font": "^1.0",
    "drupal/colorbutton": "^1.1",
    "drupal/config_installer": "^1.8",
    "drupal/core": "8.6.4",
    "drupal/easy_breadcrumb": "^1.8",
    "drupal/editor_advanced_link": "^1.4",
    "drupal/embed": "^1.0",
    "drupal/entity_embed": "^1.0@beta",
    "drupal/facets": "^1.2",
    "drupal/field_group": "^1.0",
    "drupal/geolocation": "^1.11",
    "drupal/imce": "^1.6",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/like_and_dislike": "^1.0@alpha",
    "drupal/linkit": "^4.3",
    "drupal/login_security": "^1.5",
    "drupal/maxlength": "^1.0@beta",
    "drupal/menu_block": "^1.5",
    "drupal/menu_force": "^1.0",
    "drupal/metatag": "^1.7",
    "drupal/module_filter": "^3.1",
    "drupal/multiselect": "^1.0",
    "drupal/password_policy": "^3.0@alpha",
    "drupal/pathauto": "^1.2",
    "drupal/recaptcha": "^2.3",
    "drupal/redirect": "^1.2",
    "drupal/restui": "^1.15",
    "drupal/search_api": "^1.11",
    "drupal/search_api_autocomplete": "^1.1",
    "drupal/search_api_glossary": "^3.4",
    "drupal/search_api_solr": "^2.3",
    "drupal/securelogin": "^1.7",
    "drupal/share_everywhere": "^1.1",
    "drupal/slick": "^1.0",
    "drupal/smtp": "^1.0@beta",
    "drupal/token": "^1.3",
    "drupal/token_filter": "^1.0",
    "drupal/twig_field_value": "^1.1",
    "drupal/twig_tweak": "^2.1",
    "drupal/views_infinite_scroll": "^1.5",
    "drupal/webform": "^5.0",
    "drush/drush": "^9.4",
    "gdsmith/jquery.easing": "1.4.1",
    "harvesthq/chosen": "^1.8",
    "kenwheeler/slick": "1.9.0",
    "vlucas/phpdotenv": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "codeception/codeception": "^2.5",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
    "droptica/codeception-drupal-bootstrap": "dev-develop",
    "droptica/codeception-drupal-storage": "dev-master",
    "droptica/codeception-drupal-users": "dev-master",
    "drupal/coder": "^8.2",
    "drupal/config_devel": "^1.2",
    "drupal/config_update": "^1.5",
    "drupal/console": "~1.0",
    "drupal/devel": "^1.2",
    "drupal/devel_debug_log": "^1.0@beta",
    "drupal/easy_install": "^10.1",
    "drupal/features": "^3.7",
    "drupal/twig_xdebug": "^1.0",
    "webflo/drupal-core-require-dev": "8.6.4"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "secure-http": false
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "cd ./web/themes/custom/drupal8_theme/ && ./scripts/symlink.sh && ./scripts/twig_functions.sh"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "cd ./web/themes/custom/drupal8_theme/ && ./scripts/symlink.sh && ./scripts/twig_functions.sh"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "patches": {
      "drupal/like_and_dislike": {
        "Changing numers of votes to percentage.": "./patches/like_dislike_percentage.patch"
      }
    },
    "drupal-scaffold": {
      "initial": {
        ".editorconfig": "../.editorconfig",
        ".gitattributes": "../.gitattributes"
      }
    }
  }
}
